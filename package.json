{
  "name": "srishti-farm",
  "version": "1.0.0",
  "description": "Full-stack web application for Srishti - The Farm retreat",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
